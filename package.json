{
  "name": "react-node-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cd src/server && npm install && npm start",
    "dev": "concurrently \"cd src/server && nodemon\" \"cd src/client && npm start\"",
    "package": "pkg . --out-path bin && cp ./src/server/node_modules/opn/xdg-open ../bin/xdg-open"
  },
  "bin": "src/server/bin/www",
  "pkg": {
    "scripts": [
      "src/client/build/**/*.js"
    ],
    "assets": [
      "src/client/**/*"
    ]
  },
  "dependencies": {},
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.13.4"
  },
  "devDependencies": {}
}
